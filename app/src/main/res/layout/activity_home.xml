<?xml version="1.0" encoding="utf-8"?>
<kaola.zhanchengguo.com.kaola.other.widget.KaolaSlidePaneLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home_kspl"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="200dp"
        android:layout_height="match_parent">

        <android.support.design.widget.NavigationView
            android:id="@+id/home_nv"
            android:layout_width="200dp"
            android:layout_height="match_parent"
            app:menu="@menu/menu_other">

        </android.support.design.widget.NavigationView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <FrameLayout
            android:id="@+id/home_content_fl"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1">


        </FrameLayout>

        <View style="@style/horizontal_line"/>

        <include
            android:id="@+id/footbar_ly"
            layout="@layout/footbar_layout" />
    </LinearLayout>

</kaola.zhanchengguo.com.kaola.other.widget.KaolaSlidePaneLayout>
